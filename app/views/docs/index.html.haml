- set_title "Q: Auto-Generated Documentation"

.row
  .col
    %h2 Auto-Generated Documentation

.row
  .col-sm-8
    - @all_docs.each do |doc_group|
      .row
        .col-sm-6
          %h3.col-sm-6= doc_group.name
        .col-sm-6
          - doc_group.docs.each do |doc|
            = link_to doc.to_s, doc.basepath

  .col-sm-4
    = local_notes
